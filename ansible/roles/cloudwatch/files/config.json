{
  "agent": {
    "run_as_user": "cwagent"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/pufferfish/stdout.log",
            "log_group_name": "pufferfish/app",
            "log_stream_name": "{instance_id}"
          },
          {
            "file_path": "/var/log/pufferfish/stderr.log",
            "log_group_name": "pufferfish/app",
            "log_stream_name": "{instance_id}"
          }
        ]
      }
    }
  }
}
